{
  "name": "@compose-generator/cli",
  "version": "0.6.0",
  "description": "Easy to use cli to generate Docker Compose YAML configuration files.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compose-generator/compose-generator.git"
  },
  "keywords": [
    "go",
    "docker",
    "cli",
    "golang",
    "yaml",
    "generator",
    "docker-compose"
  ],
  "author": "Marc Auberer",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/compose-generator/compose-generator/issues"
  },
  "homepage": "https://www.compose-generator.com",
  "goBinary": {
    "name": "compose-generator",
    "path": "./bin",
    "url": "https://github.com/compose-generator/compose-generator/releases/download/{{version}}/compose-generator_{{version}}_{{platform}}_{{arch}}.tar.gz"
  },
  "dependencies": {
    "go-npm": "^0.1.9"
  }
}
